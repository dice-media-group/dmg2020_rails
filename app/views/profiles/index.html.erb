


<div class="row">
  <div class="col-sm-6">
    <h1>Profiles</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_profile_path, class: 'btn btn-primary' do %>
    Add New Profile
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>City</th>
            <th>State</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @profiles.each do |profile| %>
        <%= content_tag :tr, id: dom_id(profile), class: dom_class(profile) do %>
          

                      <td><%= profile.city %></td>
                      <td><%= profile.state %></td>
          
          
            <td><%= link_to 'Show', profile %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
